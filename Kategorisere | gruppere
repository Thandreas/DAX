Saker kategorisert p√• antall avslag = 
VAR EventCount = 
    CALCULATE(
        COUNTROWS(PM_SAKER_POL), 
        ALLEXCEPT(PM_SAKER_POL, 
        PM_SAKER_POL[SaksID])
    )

RETURN
    SWITCH(
        TRUE(),
        EventCount <=5, FORMAT(EventCount, "0"),
        TRUE(), "> 5"
    )

SortOrderColumn antall policytreff = 
VAR EventCount = 
    CALCULATE(
        COUNTROWS(PM_SAKER_POL),
        ALLEXCEPT(PM_SAKER_POL, PM_SAKER_POL[SaksID])
    )
RETURN
    IF(
        EventCount <= 5, 
        FORMAT(EventCount, "0"), 
        "6"
    )
